<% if partials.length == 1 %>
  <div class="row-fluid single-form">
    <div class="span3">
    </div>
    <div class="span6">
      <%= partial partials[0] %>
    </div>
  </div>
<% else %>
<div class="row-fluid">
  <div class="span6">
    <%= partial partials[0] %>
  </div>
  <div class="span6">
    <%= partial partials[1] %>
  </div>
</div>
<% end %>
