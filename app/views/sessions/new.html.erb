<% modal_dialog I18n.t("identity.form.title") do %>
  <% modal_body do %>
    <% accordion do %>
      <% @partials.each do |partial, index| %>
        <% accordion_group(partial) do %>
          <%= render "sessions/forms/#{partial}" %>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
<% end %>
