<ul id="comments" class="comments list">
  <% unless comments.empty? %>
    <%= render :partial => 'opinio/comments/comment', :collection => comments %>
  <% else %>
    <li id="no_comments"><%= t('comment.messages.no_comments_found') %></li>
  <% end %>
</ul>

